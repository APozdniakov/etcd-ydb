{
  "name": "etcd-ydb-devcontainer",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "etcd-ydb",
  "runServices": [
    "etcd",
    "ydb"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "llvm-vs-code-extensions.vscode-clangd",
        "ms-vscode.cmake-tools",
        "ms-vscode.cpptools",
        "ms-vscode.cpptools-extension-pack",
        "ms-python.python"
      ]
    }
  },
  "forwardPorts": [
    22379,
    22380
  ],
  "remoteUser": "builder",
  "updateRemoteUserUID": false,
  "workspaceFolder": "/workspaces/etcd-ydb",
  "shutdownAction": "stopCompose"
}
